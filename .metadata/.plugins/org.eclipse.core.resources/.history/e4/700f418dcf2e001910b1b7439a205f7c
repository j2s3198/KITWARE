package it.kit.gware.calendar;

import java.util.List;

import dto.Calendar_Dto;

public class Cal_ServiceImpl implements Calendar_Service {

	Calendar_Dao cal_Dao;
	
	@Override
	public void addCal(Calendar_Dto calDto) {
		cal_Dao.insertCal(calDto);
	}

	@Override
	public void removeCal(int calendar_id) {
		cal_Dao.deleteCal(calendar_id);
	}

	@Override
	public void modifyCal(Calendar_Dto calDto) {
		cal_Dao.updateCal(calDto);
	}

	@Override
	public List<Calendar_Dto> getAllCal() {
		return cal_Dao.selectAllCal();
	}

}
